{
  "template": "monthly-file_download-*",
  "settings": {
    "index": {
      "refresh_interval": "1m"
    }
  },
  "mappings": {
    "file_download_aggregation": {
      "_source": {
        "enabled": false
      },
      "_all": {
        "enabled": false
      },
      "date_detection": false,
      "numeric_detection": false,
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "count": {
          "type": "integer",
          "index": "not_analyzed"
        },
        "bucket": {
          "type": "string",
          "index": "not_analyzed"
        },
        "key": {
          "type": "string",
          "index": "not_analyzed"
        },
        "collection": {
          "type": "string",
          "index": "not_analyzed"
        }
      }
    },
    "file_download_bookmark": {
      "date_detection": false,
      "properties": {
        "date": {
          "type": "date"
        }
      }
    }
  },
  "aliases": {
    "monthly-file_download": {}
  }
}
